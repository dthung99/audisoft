				{
					"name": "Login",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"// Extract accessToken and refreshToken from response\r",
									"var jsonData = pm.response.json();\r",
									"\r",
									"// Check if the response contains the expected structure\r",
									"if (jsonData && jsonData.data && jsonData.data.accessToken && jsonData.data.refreshToken) {\r",
									"    // Set accessToken as environment variable\r",
									"    pm.environment.set(\"accessToken\", jsonData.data.accessToken);\r",
									"    \r",
									"    // Set refreshToken as environment variable\r",
									"    pm.environment.set(\"refreshToken\", jsonData.data.refreshToken);\r",
									"    \r",
									"    // Log success message to Postman console\r",
									"    console.log(\"Tokens extracted and saved to environment variables\");\r",
									"    console.log(\"Access Token: \" + jsonData.data.accessToken.substring(0, 20) + \"...\");\r",
									"    console.log(\"Refresh Token: \" + jsonData.data.refreshToken.substring(0, 20) + \"...\");\r",
									"} else {\r",
									"    // Log error if the expected structure is not found\r",
									"    console.error(\"Failed to extract tokens. Response structure doesn't match expected format.\");\r",
									"}"
